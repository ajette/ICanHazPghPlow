{
  "manifest_version": 2,

  "name": "ICanHazPghPlow",
  "short_name": "HazPghPlow",
  "description": "Adds a button that lets you view all Pittsburgh Plow Data",
  "version": "1.0.2",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["http://city.temeda.com/*"],
      "js": ["jquery-1.11.2.min.js", "snowplowall.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "http://city.temeda.com/*"
  ]
}